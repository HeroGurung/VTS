<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sss.vts">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/khukri"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/khukri"
        android:supportsRtl="true"
        android:theme="@style/AppThemeRegister">
        <activity android:name=".User.UserActivity" />
        <activity
            android:name=".Login.LoginActivity"
            android:theme="@style/AppThemeLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Admin.AdminActivity" />
        <activity
            android:name=".User.RegisterActivity"
            android:theme="@style/AppThemeRegister" />
    </application>

</manifest>